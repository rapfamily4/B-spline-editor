; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Draggable",
"language": "GDScript",
"path": "res://scenes/draggable.gd"
}, {
"base": "NumberDisplay",
"class": "KnotDisplay",
"language": "GDScript",
"path": "res://scenes/ui/knot_display.gd"
}, {
"base": "HBoxContainer",
"class": "NumberDisplay",
"language": "GDScript",
"path": "res://scenes/ui/number_display.gd"
}, {
"base": "Node2D",
"class": "Spline",
"language": "GDScript",
"path": "res://scenes/spline/spline.gd"
}, {
"base": "CanvasLayer",
"class": "UI",
"language": "GDScript",
"path": "res://scenes/ui/ui.gd"
} ]
_global_script_class_icons={
"Draggable": "",
"KnotDisplay": "",
"NumberDisplay": "",
"Spline": "",
"UI": ""
}

[application]

config/name="Generalized Spline Editor (compatibility)"
config/description="Simple demo in which you can edit a B-spline evaluated with the De Boor - Cox algorithm. This version of the editor was remade in Godot 3.5 for better compatibility with browsers."
run/main_scene="res://scenes/editor.tscn"
config/icon="res://icons/512.png"

[display]

window/size/height=720

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
