; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Generalized Spline Editor"
config/description="Simple demo in which you can edit a B-spline evaluated with the De Boor - Cox algorithm."
config/version="1.0"
run/main_scene="res://scenes/editor.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")

[display]

window/size/viewport_width=1024
window/size/viewport_height=720

[dotnet]

project/assembly_name="Generalized Spline Editor"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
